{
	"definitions": {},
	"$schema": "http://json-schema.org/draft-07/schema#", 
	"$id": "https://example.com/object1604302976.json", 
	"title": "Root", 
	"type": "object",
	"required": [
		"Request"
	],
	"properties": {
		"Request": {
			"$id": "#root/Request", 
			"title": "Request", 
			"type": "object",
			"required": [
				"requestId",
				"timeStamp",
				"action",
				"username",
				"sourceNode",
				"feature",
				"userid",
				"dataset"
			],
			"properties": {
				"requestId": {
					"$id": "#root/Request/requestId", 
					"title": "Requestid", 
					"type": "string",
					"default": "",
					"minLength":1,
					"examples": [
						"18945678920071"
					],
					"pattern": "[^ ,null]"
				},
				"timeStamp": {
					"$id": "#root/Request/timeStamp", 
					"title": "Timestamp", 
					"type": "string",
					"default": "",
					"examples": [
						"Thus Oct 18 20:06:38 IST 2019"
					],
					"pattern": "^.*$"
				},
				"action": {
					"$id": "#root/Request/action", 
					"title": "Action", 
					"type": "string",
					"default": "",
					"examples": [
						"UpdateOrder"
					],
					"pattern": "^.*$"
				},
				"username": {
					"$id": "#root/Request/username", 
					"title": "Username", 
					"type": "string",
					"default": "",
					"examples": [
						"crmadmin"
					],
					"pattern": "^.*$"
				},
				"sourceNode": {
					"$id": "#root/Request/sourceNode", 
					"title": "Sourcenode", 
					"type": "string",
					"default": "",
					"examples": [
						"CRM"
					],
					"pattern": "^.*$"
				},
				"feature": {
					"$id": "#root/Request/feature", 
					"title": "Feature", 
					"type": "string",
					"default": "",
					"examples": [
						"UpdateOrder"
					],
					"pattern": "^.*$"
				},
				"userid": {
					"$id": "#root/Request/userid", 
					"title": "Userid", 
					"type": "string",
					"default": "",
					"examples": [
						"1"
					],
					"pattern": "^.*$"
				},
				"dataset": {
					"$id": "#root/Request/dataset", 
					"title": "Dataset", 
					"type": "object",
					"required": [
						"param"
					],
					"properties": {
						"param": {
							"$id": "#root/Request/dataset/param", 
							"title": "Param", 
							"type": "array",
							"default": [],
							"items":{
								"$id": "#root/Request/dataset/param/items", 
								"title": "Items", 
								"type": "object",
								"required": [
									"id",
									"value"
								],
								"properties": {
									"id": {
										"$id": "#root/Request/dataset/param/items/id", 
										"title": "Id", 
										"type": "string",
										"default": "",
										"examples": [
											"order_id"
										],
										"pattern": "^.*$"
									},
									"value": {
										"$id": "#root/Request/dataset/param/items/value", 
										"title": "Value", 
										"type": "string",
										"default": "",
										"examples": [
											"11307775448024"
										],
										"pattern": "^.*$"
									}
								}
							}

						}
					}
				}

			}
		}

	}
}
